<?xml version="1.0" encoding="UTF-8"?>
<config>
	<global>
		<entity>
			<iwe_ratings>
                <entity>
                    <table>iwe_stat_entity</table>
                    <key>id</key>
                    <attributes>
                        <id translate="label">
                            <label>ID</label>
                            <type>number</type>
                        </id>
                        <year translate="label">
                            <label>Year</label>
                            <type>number</type>
                        </year>
                        <school_name translate="label">
                            <label>School name</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_name>
                        <school_type translate="label">
                            <label>School type</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_type>
                        <school_district translate="label">
                            <label>School district</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_district>
                        <school_region translate="label">
                            <label>School region</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_region>
                        <subject translate="label">
                            <label>Subject</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </subject>
                        <way translate="label">
                            <label>Way</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </way>
                        <passed_number translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                        </passed_number>
                        <interval1 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <description>100-123</description>
                            <nullable>1</nullable>
                        </interval1>
                        <interval2 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>124-135</description>
                        </interval2>
                        <interval3 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>136-150</description>
                        </interval3>
                        <interval4 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>150-161</description>
                        </interval4>
                        <interval5 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>162-172</description>
                        </interval5>
                        <interval6 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>173-183</description>
                        </interval6>
                        <interval7 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>183-190</description>
                        </interval7>
                        <interval8 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>190-195</description>
                        </interval8>
                        <interval9 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>195-200</description>
                        </interval9>
                    </attributes>
                </entity>
				<rating>
					<table>iwe_school_stat</table>
					<key>id</key>
					<attributes>
						<id translate="label">
							<label>ID</label>
							<type>number</type>
						</id>
						<school_id translate="label,description">
							<label>School</label>
                            <type>select</type>
                            <options>iwe_school/school</options>
                            <required>1</required>
                            <description>School name and school town</description>
						</school_id>
						<subject_id translate="label">
							<label>Subject</label>
                            <type>select</type>
                            <options>iwe_ratings/subject</options>
                            <required>1</required>
                            <description>Name of examination subjects </description>
                        </subject_id>
                        <rate translate="label,description">
                            <label>Rating</label>
                            <type>number</type>
                            <data_type>decimal</data_type>
                            <scale>2</scale>
                            <precision>12</precision>
                            <required>1</required>
                            <description>Rate for current school</description>
                        </rate>
                        <count translate="label,description">
                            <label>Passed number</label>
                            <type>number</type>
                            <data_type>decimal</data_type>
                            <scale>2</scale>
                            <precision>12</precision>
                            <required>1</required>
                            <description>Number of students who passed</description>
                        </count>
                        <year translate="label,description" >
                            <label>Year</label>
                            <type>number</type>
                            <required>1</required>
                            <description>Year of examination</description>
                        </year>
                        <info translate="label,description">
                            <label>Detail ratings</label>
                            <data_type>text</data_type>
                            <serializable>1</serializable>
                            <no_view>1</no_view>
                            <nullable>1</nullable>
                            <description>Detail school rating</description>
                        </info>
					</attributes>
				</rating>
                <subject>
                    <table>iwe_ratings_subjects</table>
                    <key>id</key>
                    <attributes>
                        <id translate="label">
                            <label>ID</label>
                            <type>number</type>
                        </id>
                        <name translate="label,description">
                            <label>Name</label>
                            <maxlength>255</maxlength>
                            <description>Subject name</description>
                            <multiview>1</multiview>
                            <required>1</required>
                        </name>
                    </attributes>
                </subject>
			</iwe_ratings>
		</entity>
	</global>
	<admin>
		<forms>
			<iwe_ratings>
				<rating translate="title,fieldset,tab">
					<title>Rating edit from</title>
					<fieldset>General</fieldset>
					<tab>General</tab>
					<source>iwe_ratings/rating</source>
					<fields>
						<school_id />
						<subject_id />
                        <rate/>
                        <count/>
                        <year/>
					</fields>
				</rating>
                <subject translate="title,fieldset,tab">
                    <title>Subject edit from</title>
                    <fieldset>General</fieldset>
                    <tab>General</tab>
                    <source>iwe_ratings/subject</source>
                    <fields>
                        <name/>
                    </fields>
                </subject>
			</iwe_ratings>
		</forms>
		<grids>
			<iwe_ratings>
				<rating translate="title">
					<title>School ratings</title>
					<source>iwe_ratings/rating</source>
                    <collection>iwe_ratings/rating</collection>
					<use_ajax>1</use_ajax>
					<fields>
						<id>	
							<width>40px</width>
						</id>
						<school_id />
                        <subject_id/>
                        <rate>
                            <output_callback>iwe_ratings/output::getValue</output_callback>
                        </rate>
                        <year/>
					</fields>
				</rating>
                <subject translate="title">
                    <title>Subject list</title>
                    <source>iwe_ratings/subject</source>
                    <collection>iwe_ratings/subject</collection>
                    <use_ajax>1</use_ajax>
                    <fields>
                        <id>
                            <width>40px</width>
                        </id>
                        <name />
                    </fields>
                </subject>
			</iwe_ratings>
		</grids>
	</admin>
</config>