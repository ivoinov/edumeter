<?xml version="1.0" encoding="UTF-8"?>
<config>
	<global>
		<entity>
			<iwe_ratings>
                <entity>
                    <table>iwe_stat_entity</table>
                    <key>id</key>
                    <attributes>
                        <id translate="label">
                            <label>ID</label>
                            <type>number</type>
                        </id>
                        <year translate="label">
                            <label>Year</label>
                            <type>number</type>
                        </year>
                        <school_name translate="label">
                            <label>School name</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_name>
                        <school_type translate="label">
                            <label>School type</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                            <nullable>1</nullable>
                        </school_type>
                        <school_district translate="label">
                            <label>School district</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_district>
                        <school_region translate="label">
                            <label>School region</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </school_region>
                        <subject translate="label">
                            <label>Subject</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </subject>
                        <way translate="label">
                            <label>Way</label>
                            <type>text</type>
                            <maxlength>255</maxlength>
                        </way>
                        <passed_number translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                        </passed_number>
                        <interval1 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <description>100-123</description>
                            <nullable>1</nullable>
                        </interval1>
                        <interval2 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>124-135</description>
                        </interval2>
                        <interval3 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>136-150</description>
                        </interval3>
                        <interval4 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>150-161</description>
                        </interval4>
                        <interval5 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>162-172</description>
                        </interval5>
                        <interval6 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>173-183</description>
                        </interval6>
                        <interval7 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>183-190</description>
                        </interval7>
                        <interval8 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>190-195</description>
                        </interval8>
                        <interval9 translate="label">
                            <label>Passed number</label>
                            <type>number</type>
                            <nullable>1</nullable>
                            <description>195-200</description>
                        </interval9>
                    </attributes>
                </entity>
                <subject_rate>
                    <table>iwe_stat_subject</table>
                    <key>id</key>
                    <attributes>
                        <id translate="label">
                            <label>ID</label>
                            <type>number</type>
                        </id>
                        <school_id translate="label">
                            <label>School</label>
                            <type>select</type>
                            <data_type>int</data_type>
                            <options>iwe_school/school</options>
                        </school_id>
                        <year translate="label">
                            <label>Year</label>
                            <type>number</type>
                        </year>
                        <subject translate="label">
                            <label>Subject</label>
                            <type>select</type>
                            <options>iwe_subject/entity</options>
                        </subject>
                        <rate translate="label">
                            <label>School rate</label>
                            <description>School rate for subject for current year</description>
                            <type>number</type>
                            <data_type>float</data_type>
                        </rate>
                        <passed_number translate="label">
                            <label>Passed numbers</label>
                            <type>number</type>
                        </passed_number>
                    </attributes>
                </subject_rate>
			</iwe_ratings>
		</entity>
	</global>
    <admin>
        <forms>
            <iwe_ratings>
                <subject_rate>
                    <title>School rates edit</title>
                    <source>iwe_ratings/subject_rate</source>
                    <use_ajax>1</use_ajax>
                    <fields>
                        <school_id/>
                        <year/>
                        <subject/>
                        <rate/>
                        <passed_number/>
                    </fields>
                </subject_rate>
            </iwe_ratings>
        </forms>
        <grids>
            <iwe_ratings>
                <subject_rate>
                    <title>School rates list</title>
                    <source>iwe_ratings/subject_rate</source>
                    <collection>iwe_ratings/subject_rate</collection>
                    <use_ajax>1</use_ajax>
                    <fields>
                        <school_id/>
                        <rate/>
                        <subject/>
                        <year/>
                    </fields>
                </subject_rate>
            </iwe_ratings>
        </grids>
    </admin>
</config>